{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train EOMT",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": [
                "fit",
                "-c", "configs/dinov2/coco/panoptic/eomt_large_640.yaml",
                "--trainer.devices", "1", 
                "--trainer.accumulate_grad_batches", "8",
                "--trainer.logger", "false",
                "--data.batch_size", "2",
                "--data.path", "coco"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
        },
        {
            "name": "Validate EOMT",
